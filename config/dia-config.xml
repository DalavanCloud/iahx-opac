<?xml version="1.0"?>
<dia_config>
	<site>trunk</site>
	<bvs_url>http://trunk.teste.bvsalud.org/</bvs_url>
	<about_bvs_url>http://bvsmodelo.bvsalud.org/</about_bvs_url>
	<home_url>http://trunk.teste.bvsalud.org/</home_url>
	<search_server>ts01dx.bireme.br:8080</search_server>
	<photocopy_url>http://scad.bvs.br/php/index.php?identificado=1</photocopy_url>
	<documents_per_page>10</documents_per_page>
	<default_lang>pt</default_lang>
	<log_dir>../../bases/logs/trunk/</log_dir>
	<environment>teste</environment>

	<search_collection_list>
		<collection>
			<name>main</name>
			<cluster_items_limit>20</cluster_items_limit>
			<set></set>
			<filter></filter>
			<index_list>
				<index>
					<name>ALL</name>
					<prefix></prefix>
				</index>
				<index>
					<name>TI</name>
					<prefix>ti</prefix>
				</index>
				<index>
					<name>AU</name>
					<prefix>au</prefix>
				</index>
				<index>
					<name>MH</name>
					<prefix>mh</prefix>
				</index>
				<index>
					<name>PT</name>
					<prefix>pt</prefix>
				</index>

			</index_list>
			<where_list>
				<where>
					<name>ALL</name>
					<filter></filter>
				</where>
				<where>
					<name>GENERAL</name>
					<filter>db:("LILACS" OR "MEDLINE_1997-2008" OR "MEDLINE_1966-1996")</filter>
				</where>
				<where>
					<level>2</level>
					<name>LILACS</name>
					<filter>db:"LILACS"</filter>
				</where>
				<where>
					<level>2</level>
					<name>IBECS</name>
					<filter>db:"IBECS"</filter>
				</where>
				<where>
					<level>2</level>
					<name>MEDLINE</name>
					<filter>db:("MEDLINE_1997-2009" OR "MEDLINE_1966-1996")</filter>
				</where>
				<where>
					<name>SPECIALIZED</name>
					<filter>db:("ADOLEC" OR "DESASTRES" OR "MedCaribe" OR "REPIDISCA")</filter>
				</where>
				<!--
            	<where>
                  <level>2</level>
                  <name>ADOLEC</name>
                  <filter>db:"ADOLEC"</filter>
                </where>
                -->
                <where>
					<level>2</level>
					<name>BBO</name>
					<filter>db:"BBO"</filter>
				</where>
				<where>
					<level>2</level>
					<name>CIDSAUDE</name>
					<filter>db:"CidSaude"</filter>
				</where>				
				<where>
					<level>2</level>
					<name>DESASTRES</name>
					<filter>db:"DESASTRES"</filter>
				</where>
                <!--
				<where>
					<level>2</level>
					<name>HISA</name>
					<filter>db:"HISA"</filter>
				</where>
                -->
				<where>
					<level>2</level>
					<name>MEDCARIB</name>
					<filter>db:"MedCarib"</filter>
				</where>
				<where>
					<level>2</level>
					<name>REPIDISCA</name>
					<filter>db:"REPIDISCA"</filter>
				</where>

				<where>
					<level>2</level>
					<name>BDENF</name>
					<filter>db:"BDEnf"</filter>
				</where>
				<where>
					<level>2</level>
					<name>HOMEOINDEX</name>
					<filter>db:"HomeoIndex"</filter>
                 </where>
				 <where>
					 <name>INTERNATIONAL</name>
					 <filter>db:("PAHO" OR "WHOLIS")</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>PAHO</name>
					 <filter>db:"PAHO"</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>WHOLIS</name>
					 <filter>db:"WHOLIS"</filter>
				 </where>
				 <where>
					 <name>SITES</name>
					 <filter>db:"LIS"</filter>
				 </where>
				 <where>
					 <name>CVSP</name>
					 <filter>db:campusvirtualsp*</filter>
				 </where>
				 <where>
					 <name>DECS</name>
					 <filter>db:"DECS"</filter>
				 </where>
				 
				 <where>
					 <name>COCHRANE</name>
					 <filter>db:("COCHRANE-REVIEWS" OR "COCHRANE-PROTOCOLS" OR "COCHRANE-CENTRAL" OR "COCHRANE-CMR" OR "COCHRANE-HTA" OR "COCHRANE-EED_BIBLIO" OR "COCHRANE-EED_ABSTRACTS" OR "COCHRANE-DARE_ABSTRACTS" OR "COCHRANE-AGENCIAS" OR "COCHRANE-BANDOLIER" OR  "COCHRANE-CLIBPLUSREFS" OR "COCHRANE-EVIDARGENT" OR "COCHRANE-GESTION" OR "COCHRANE-KOVACS")</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>COCHRANE-REVIEWS</name>
					 <filter>db:"COCHRANE-REVIEWS"</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>COCHRANE-PROTOCOLS</name>
					 <filter>db:"COCHRANE-PROTOCOLS"</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>COCHRANE-CENTRAL</name>
					 <filter>db:"COCHRANE-CENTRAL"</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>COCHRANE-CMR</name>
					 <filter>db:"COCHRANE-CMR"</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>COCHRANE-HTA</name>
					 <filter>db:"COCHRANE-HTA"</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>COCHRANE-EED_BIBLIO</name>
					 <filter>db:"COCHRANE-EED_BIBLIO"</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>COCHRANE-EED_ABSTRACTS</name>
					 <filter>db:"COCHRANE-EED_ABSTRACTS"</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>COCHRANE-DARE_ABSTRACTS</name>
					 <filter>db:"COCHRANE-DARE_ABSTRACTS"</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>COCHRANE-AGENCIAS</name>
					 <filter>db:"COCHRANE-AGENCIAS"</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>COCHRANE-BANDOLIER</name>
					 <filter>db:"COCHRANE-BANDOLIER"</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>COCHRANE-CLIBPLUSREFS</name>
					 <filter>db:"COCHRANE-CLIBPLUSREFS"</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>COCHRANE-EVIDARGENT</name>
					 <filter>db:"COCHRANE-EVIDARGENT"</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>COCHRANE-GESTION</name>
					 <filter>db:"COCHRANE-GESTION"</filter>
				 </where>
				 <where>
					 <level>2</level>
					 <name>COCHRANE-KOVACS</name>
					 <filter>db:"COCHRANE-KOVACS"</filter>
				 </where>
			 </where_list>
			 <sort_list>
				 <sort>
					 <name>RELEVANCE</name>
					 <value>weight desc,score desc</value>
				 </sort>
				 <sort>
				 	 <default_for_empty_query>1</default_for_empty_query>
					 <name>YEAR_DESC</name>
					 <value>da desc</value>
				 </sort>
				 <sort>
					 <name>YEAR_ASC</name>
					 <value>da asc</value>
				 </sort>
				 <sort>
					 <name>SJR</name>
					 <value>order_sjr desc</value>
				 </sort>
			 </sort_list>
             <format_list>
               <format>
                 <name>summary</name>
               </format>
               <format>
                 <name>simple</name>
               </format>
             </format_list>
		</collection>
	</search_collection_list>
</dia_config>
